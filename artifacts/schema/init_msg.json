{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "InitMsg",
  "type": "object",
  "required": [
    "anchor_contract",
    "aterra_contract",
    "block_time",
    "lottery_interval",
    "owner",
    "prize_distribution",
    "reserve_factor",
    "split_factor",
    "stable_denom",
    "ticket_prize",
    "unbonding_period"
  ],
  "properties": {
    "anchor_contract": {
      "$ref": "#/definitions/HumanAddr"
    },
    "aterra_contract": {
      "$ref": "#/definitions/HumanAddr"
    },
    "block_time": {
      "type": "integer",
      "format": "uint64",
      "minimum": 0.0
    },
    "lottery_interval": {
      "type": "integer",
      "format": "uint64",
      "minimum": 0.0
    },
    "owner": {
      "$ref": "#/definitions/HumanAddr"
    },
    "prize_distribution": {
      "type": "array",
      "items": {
        "$ref": "#/definitions/Decimal256"
      }
    },
    "reserve_factor": {
      "$ref": "#/definitions/Decimal256"
    },
    "split_factor": {
      "$ref": "#/definitions/Decimal256"
    },
    "stable_denom": {
      "type": "string"
    },
    "ticket_prize": {
      "$ref": "#/definitions/Decimal256"
    },
    "unbonding_period": {
      "type": "integer",
      "format": "uint64",
      "minimum": 0.0
    }
  },
  "definitions": {
    "Decimal256": {
      "description": "A fixed-point decimal value with 18 fractional digits, i.e. Decimal256(1_000_000_000_000_000_000) == 1.0 The greatest possible value that can be represented is 115792089237316195423570985008687907853269984665640564039457.584007913129639935 (which is (2^128 - 1) / 10^18)",
      "type": "string"
    },
    "HumanAddr": {
      "type": "string"
    }
  }
}
