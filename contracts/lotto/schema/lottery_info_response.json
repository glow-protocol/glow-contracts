{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "LotteryInfoResponse",
  "type": "object",
  "required": [
    "awarded",
    "lottery_id",
    "number_winners",
    "page",
    "prize_buckets",
    "rand_round",
    "sequence",
    "timestamp"
  ],
  "properties": {
    "awarded": {
      "type": "boolean"
    },
    "lottery_id": {
      "type": "integer",
      "format": "uint64",
      "minimum": 0.0
    },
    "number_winners": {
      "type": "array",
      "items": {
        "type": "integer",
        "format": "uint32",
        "minimum": 0.0
      },
      "maxItems": 6,
      "minItems": 6
    },
    "page": {
      "type": "string"
    },
    "prize_buckets": {
      "type": "array",
      "items": {
        "$ref": "#/definitions/Uint256"
      },
      "maxItems": 6,
      "minItems": 6
    },
    "rand_round": {
      "type": "integer",
      "format": "uint64",
      "minimum": 0.0
    },
    "sequence": {
      "type": "string"
    },
    "timestamp": {
      "type": "integer",
      "format": "uint64",
      "minimum": 0.0
    }
  },
  "definitions": {
    "Uint256": {
      "type": "string"
    }
  }
}
